	<body>



<% // prob[i].idd , prob[i].ind %>
<% var len = prob.length ;%>
<%if(len === 0){%>
	
	
	<div class = "container">
		<div class="jumbotron text-center ">
		  <h1> 
			NO PROBLEMS IN THIS RANGE... 
			Please Modify the range 
			<a class="btn btn-primary" href="/precal" role="button">Click here to modify the range </a>
		</h1>
		</div>
	</div>

<%}%>



<%if(len != 0 ){%>

	
	
	


<div class = "container">
	<nav class="navbar fixed-top navbar-dark  " style="background-color: #2c3e50;">
		<a class="navbar-brand" href = "/" style = "font-family: 'Chilanka', cursive;">Home Page</a>
	</nav>
	<br>
	<br>
	<br>
	<div id = "vc">
		<div class="row">
			<h4 class="col-lg-12" style = "font-family: 'Sriracha', cursive; text-align = center">
				List of problems available for the user in the desired difficulty range  
			</h4>
		</div>
	</div>
</div>








	<table  class = "table table-striped table-dark table-bordered "   >
	  <thead class = "thead-dark">
		<tr>
		  <th scope = "col">Serial Number</th>
		  <th scope = "col">Contest Id</th>
		  <th scope = "col">Problem Index</th>
		  <th scope = "col">Rating</th>
		  <th scope = "col">Name and link</th>
		</tr>
	  </thead>
	  <tbody>
		<% for(var i = 0 ; i < len ; i ++ ){%>
		<%	var x = prob[i].idd %>
		<%	var y = prob[i].ind %>
		<%	var z = prob[i].titl %>
		<%	var url = "https://codeforces.com/contest/" + x + "/problem/" + y %>
		<tr>
		  <th scope = "row"><%= i+1 %></th>
			<td><%= x %></td>
			<td><%= y %></td>
			<td><%= prob[i].rat %></td>
			<td><a href = <%= url %>  >
				<%= (prob[i].titl) %>
			</a> </td>
		  </tr>

	<%}%>








	  </tbody>
		</table>
<%}%>

