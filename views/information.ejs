	<% var rate = obj.data["result"][ (obj.data["result"].length) - 1 ]["newRating"] %>
	
	<% var mx = 0 %>
	<% for(var i = 0; i <= (Number)(obj.data["result"] .length)-1 ; i ++ ){%>
		<%if(obj.data["result"][ (Number)(i) ]["newRating"] > mx ){%>
			<%mx = obj.data["result"][ (Number)(i) ]["newRating"] ;%>
	<%}%>
	<% }%>
	
	
	<%// navbar %>
	
<nav class="navbar fixed-top navbar-dark  " style="background-color: #2c3e50;">
	  
	  
	<a class="navbar-brand" href = "/" style = "font-family: 'Chilanka', cursive;">Home Page</a>
