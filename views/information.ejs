	<div  >
		<div class="jumbotron text-center text-white" style="background-color: 	SteelBlue;">
		    <h2 class="display-4" style= "font-family: 'Sriracha', cursive;">Rating Information of User</h2>
			<h2 >Username       :  <%= obj.username %></h2>
		    <h2 >Current Rating :  <%= rate %></h2>
			<h2 >Maximum Rating :  <%= mx %></h2>
		</div>
	</div>
	
	
	
	
	
	
	
	

<br> 

	<div class = "container">
		<% // graph %>
			  <canvas id="chart" ></canvas>
			  <script>
			var ctx = document.getElementById('chart').getContext('2d');
			var myChart = new Chart(ctx, {
				type: 'line',
				data: {
					labels: <%- JSON.stringify(obj.xlabel) %>,
					datasets: [{
						label: 'Rating Curve',
						data: <%- JSON.stringify(obj.ylabel) %>,
						backgroundColor: 'rgba(153, 0, 255, 0.4)',
						borderColor: 'rgba(102, 0, 102, 1)',
						borderWidth: 1
					}]
				},
				options: {
					scales: {
						yAxes: [{
							ticks: {
								beginAtZero: true
							}
						}]
					}
				}
		  });
		  </script>
	</div>
		
	  
	  
	  
	  
	

	
	
	
	
	
	
	
	
	
	

